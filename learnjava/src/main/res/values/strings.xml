    <string name="clip_sync_test_description">You can test if the syncing works using this dummy code sample:</string>
    <string name="dummy_code_sample" translatable="false"><![CDATA[
     <font color="#E65100">public static</font> <font color="#DF7401">void</font> main(<font color="#999900">String</font>[] args) {
<br/>	&nbsp;&nbsp;&nbsp;&nbsp;<font color="#999900">System</font>.out.println(\"ClipSync!\");
<br/>}
    ]]></string>
