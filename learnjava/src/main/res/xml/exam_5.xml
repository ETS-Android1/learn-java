    <!-- Questions about 'What is Java?' -->
    <question type="multi_choice">
        <text>When we say 'Java', what can we mean by it?</text>
        <answer>A platform for software development</answer>
        <answer>A database</answer>
        <answer>A programming language</answer>
        <answer>Bytecode</answer>
        <correct>0</correct>
